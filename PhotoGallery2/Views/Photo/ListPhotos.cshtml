@model IEnumerable<PhotoGallery2.Models.Photo>
@{
    ViewBag.Title = "ListPhotos";
}

<div class="panel panel-primary">
    <div class="panel panel-heading">
        <h2 class="panel-title">List of Photos</h2>        
    </div>
    <div id="listPhotos" class="panel-body">
        @{
            Html.RenderPartial("_ListPhotos", Model);
        }
    </div>
    <div class="panel-footer">
        <button class="btn btn-danger" type="button" id="btnDelete">
            <span class="glyphicon glyphicon-trash"></span>
            Delete
        </button>
    </div>
</div>
@Html.AntiForgeryToken()

@section scripts
{
    <script type="text/javascript" src="~/Scripts/PhotoGallery/GlobalFunctions.js"></script> 
}
